{
  "version": 2,
  "robot": {
    "modules": {
      "gps": {
          "driver": "gps",
          "in": ["raw"],
          "out": ["position"],
          "init": {}
      },
      "gps_serial": {
          "driver": "serial",
          "in": [],
          "out": ["raw"],
          "init": {"port": "/dev/ttyUSB0", "speed": 4800}
      },
      "localization": {
	      "driver": "lsqr_loc:LSqrLoc",
	      "in": ["nmea_data (poznamka)"],
	      "out": ["pose3d (poznamka)"],
	      "init":{
            "gps_sd": [0.03, 0.03, 0.09]
          }
      },
       "oak_d": {
          "driver": "osgar.drivers.oak_camera:OakCamera",
          "init": {
            "fps": 5,
            "is_color": true,
            "is_depth": true,
            "cam_ip": "192.168.1.52",

            "mono_resolution": "THE_400_P",
            "color_resolution": "THE_1080_P",
            "color_manual_focus": 130,

            "stereo_median_filter": "KERNEL_7x7",
            "stereo_mode": "HIGH_DENSITY",
            "stereo_extended_disparity": true,
            "stereo_subpixel": false,
            "stereo_left_right_check": false
          }
      },
       "oak_1": {
          "driver": "osgar.drivers.oak_camera:OakCamera",
          "init": {
            "fps": 5,
            "is_color": true,
            "cam_ip": "192.168.1.61",
            "color_resolution": "THE_1080_P"
          }
      }
    },
    "links": [
              ["kloubak.pose2d", "localization.odom"],
              ["imu.orientation", "localization.orientation"],
              ["gps.nmea_data", "localization.nmea_data"]
    ]
  }
}
